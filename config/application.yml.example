spring:
  datasource:
    url: jdbc:mysql://localhost:3306/lic?useUnicode=true&characterEncoding=UTF-8
    username: root
    password: 123456
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.cj.jdbc.Driver
    druid:
      filter:
        stat:
          enabled: true
          db-type: mysql
          log-slow-sql: true
          slow-sql-millis: 2000

  redis:
    host: 127.0.0.1
    port: 6379
    password:
    max-total: 4
    max-idle: 4
    min-idle: 4
    max-wait-millis: 1000

  mail:
    host:
    port:
    username:
    password:

  jpa:
    database: mysql
    show-sql: false
    hibernate:
      ddl-auto: update
    database-platform: org.hibernate.dialect.MySQL5InnoDBDialect
  http:
    encoding:
      charset: UTF-8