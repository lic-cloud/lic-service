spring:
  datasource:
    url: jdbc:mysql://127.0.0.1:3306/lic?useUnicode=true&characterEncoding=UTF-8
    username: root
    password:
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.cj.jdbc.Driver
    druid:
      filter:
        stat:
          enabled: true
          db-type: mysql
          log-slow-sql: true
          slow-sql-millis: 2000
  redis:
    host: 127.0.0.1
    port: 6379
    password:
  mail:
    host:
    port:
    username:
    password:
  jpa:
    database: mysql
    show-sql: false
    hibernate:
      ddl-auto: create-drop
  proxy:
    trusts: "127.0.0.1/8,::1/128"